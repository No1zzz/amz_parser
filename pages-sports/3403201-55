	P.when('jQuery', 'ready').execute(function($) {
		P.when('search-page-access').execute(function() {
			var successFlag = 0;
			SPUtils.afterEvent('spATFEvent', function () {
				if (!successFlag) {
					var browserWidth = $(window).width();
					var browserHeight = $(window).height();
					
					// Make a request to the session cache update handler in Gurupa
					$.post('/gp/product/sessionCacheUpdateHandler.html/186-2328070-0729851', 
						 { 'sessionCacheUpdateFlag' : '1',
						   'pageType'               : 'Search',
						   'browserWidth'           : browserWidth,
						   'browserHeight'          : browserHeight,
						   'token'                  : "fVoboEDsYMjWCjzTN8sCOWL0iXpmIAkzWqn3smX5bjI="
						 },
						 function(data) {
						 	successFlag++;
						 }
					);
				}
			});
		});
	});
}, 25);
